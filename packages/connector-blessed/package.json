{
  "name": "@vuminal/connector-blessed",
  "version": "0.0.3",
  "description": "connecting vuminal to blessed",
  "author": "Jason Yu <me@ycmjason.com>",
  "homepage": "https://github.com/ycmjason/vuminal/tree/master/packages/connector-blessed",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycmjason/vuminal.git"
  },
  "scripts": {
    "types": "tsc --noEmit",
    "build:typeDef": "tsc --declaration --outDir dist --emitDeclarationOnly --declarationMap",
    "build:rollup": "rollup -c",
    "build:rollup:watch": "rollup -c -w",
    "prebuild": "/bin/rm -rf dist",
    "build": "npm run build:typeDef && npm run build:rollup",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "bugs": {
    "url": "https://github.com/ycmjason/vuminal/issues"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.0.0",
    "@vue/shared": "^3.0.0",
    "blessed": "^0.1.81"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.17",
    "vuminal": "^0.0.5"
  }
}
